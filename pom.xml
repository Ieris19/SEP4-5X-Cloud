<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>dk.via.sep4</groupId>
    <artifactId>cloud</artifactId>
    <version>0.0.1-SNAPSHOT</version>

    <name>cloud</name>
    <description>cloud</description>

    <properties>
        <sep4.source.repoURL>https://github.com/Ieris19/IerisLib/tree/prime</sep4.source.repoURL>
        <sep4.java.version>17</sep4.java.version>
        <sep4.encoding>UTF-8</sep4.encoding>
        <maven.compiler.source>${sep4.java.version}</maven.compiler.source>
        <maven.compiler.target>${sep4.java.version}</maven.compiler.target>
        <project.build.sourceEncoding>${sep4.encoding}</project.build.sourceEncoding>
        <project.reporting.outputEncoding>${sep4.encoding}</project.reporting.outputEncoding>

        <sep4.dependencyVersion.spring>3.0.6</sep4.dependencyVersion.spring>
        <sep4.dependencyVersion.lombok>1.18.26</sep4.dependencyVersion.lombok>
    </properties>
    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-data-mongodb</artifactId>
            <version>${sep4.dependencyVersion.spring}</version>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
            <version>${sep4.dependencyVersion.spring}</version>
        </dependency>

        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
            <version>${sep4.dependencyVersion.lombok}</version>
            <optional>true</optional>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-test</artifactId>
            <version>${sep4.dependencyVersion.spring}</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>commons-codec</groupId>
            <artifactId>commons-codec</artifactId>
            <version>1.12</version>
        </dependency>
        <dependency>
            <groupId>org.json</groupId>
            <artifactId>json</artifactId>
            <version>20230227</version>
        </dependency>
        <dependency>
            <groupId>org.thethingsnetwork</groupId>
            <artifactId>data-mqtt</artifactId>
            <version>2.1.3</version>
        </dependency>
        <dependency>
            <groupId>org.eclipse.paho</groupId>
            <artifactId>org.eclipse.paho.client.mqttv3</artifactId>
            <version>1.2.5</version>
        </dependency>
        <dependency>
            <groupId>org.mongodb</groupId>
            <artifactId>mongodb-driver-sync</artifactId>
            <version>4.9.1</version>
        </dependency>
    </dependencies>
</project>
